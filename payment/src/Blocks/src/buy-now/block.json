{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "woo-vipps/buy-now",
	"version": "3.0.0",
	"title": "Vipps MobilePay Buy Now",
	"attributes": {
		"hasProductContext": {
			"type": "boolean",
			"default": false
		},
		"productId": {
			"type": "string",
			"default": ""
		},
		"productName": {
			"type": "string",
			"default": ""
		},
		"showProductSelection": {
			"type": "boolean",
			"default": "true"
		},
		"variant": {
			"type": "string",
			"default": "default-mini"
		},
		"language": {
			"type": "string",
			"default": "store"
		}
	},
	"keywords": [
		"WooCommerce",
		"woo-gutenberg-products-block",
		"Vipps",
		"Vipps Express",
		"Express",
		"buy-now"
	],
	"category": "woocommerce",
	"usesContext": ["postId", "postType"],
	"supports": {
		"html": false
	},
	"textdomain": "woo-vipps",
	"editorScript": "file:./index.js",
	"render": "file:./render.php"
}
