{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "woo-vipps/buy-now",
	"version": "2.1.0",
	"title": "Vipps MobilePay Buy Now",
	"attributes": {
		"hasProductContext": {
			"type": "boolean",
			"default": false
		},
		"productId": {
			"type": "number",
			"default": null
		},
		"productName": {
			"type": "string",
			"default": null
		},
		"showProductSelection":{
			"type": "boolean",
			"default": "true"
		},
		"variant": {
			"type": "string",
			"default": "default-mini"
		},
		"language": {
			"type": "string",
			"default": "store"
		}
	},
	"keywords": [
		"WooCommerce",
		"woo-gutenberg-products-block",
		"Vipps",
		"Vipps Express",
		"Express",
		"buy-now"
	],
	"category": "woocommerce",
	"usesContext": [
		"query",
		"queryId",
		"postId"
	],
	"supports": {
		"html": false
	},
	"textdomain": "woo-vipps",
	"editorScript": "file:./index.js",
	"render": "file:./render.php"
}
